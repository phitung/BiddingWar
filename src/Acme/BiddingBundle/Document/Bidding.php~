<?php
// src/Acme/BiddingBundle/Document/Bidding.php
namespace Acme\BiddingBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Bidding
{
	/**
	 * @MongoDB\Id
	 */
	protected $id;

	/**
	 * @MongoDB\String
	 */
	protected $name;

	/**
	 * @MongoDB\Float
	 */
	protected $price;
	
	/**
	 * @MongoDB\Date
	 */
	protected $time;
	
}	